{
    "compilerOptions": {
        "target": "esnext",
        "module": "esnext",
        "lib": ["esnext", "dom"],
        "jsx": "react-jsx",
        "moduleResolution": "node",
        "allowJs": true,
        "strict": false,
        "sourceMap": true,
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "baseUrl": "./wp-content/themes/wpstart/src",
        "paths": {
            "@blocks/*": ["Blocks/resources/*"]
        }
    },
    "include": ["./wp-content/themes/wpstart/src", "./globals.d.ts"]
}
