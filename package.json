{
    "name": "wp-start",
    "private": true,
    "scripts": {
        "build": "npm run clean && npm run generate:types && wp-scripts build",
        "clean": "scripty",
        "client": "scripty",
        "config": "scripty",
        "generate:types": "node ./scripts/build/createBlockAttributeTypes.js",
        "postinstall": "scripty",
        "pull:staging": "scripty",
        "push:e2e": "scripty",
        "push:staging": "scripty",
        "server": "scripty",
        "ssh:e2e": "scripty",
        "ssh:staging": "scripty",
        "start": "concurrently \"npm run server\" \"npm run client\"",
        "storybook": "storybook dev -p 6006",
        "storybook:build": "storybook build",
        "test": "node ./scripts/test/runTests.js",
        "test:install": "scripty",
        "unreleased": "scripty"
    },
    "dependencies": {
        "@wordpress/block-editor": "^11.5.0",
        "@wordpress/blocks": "^12.5.0",
        "@wordpress/components": "^23.5.0"
    },
    "devDependencies": {
        "@jest/globals": "^29.6.1",
        "@storybook/addon-essentials": "^7.2.2",
        "@storybook/addon-interactions": "^7.2.2",
        "@storybook/addon-links": "^7.2.2",
        "@storybook/addon-onboarding": "^1.0.8",
        "@storybook/blocks": "^7.2.2",
        "@storybook/react": "^7.2.2",
        "@storybook/react-webpack5": "^7.2.2",
        "@storybook/testing-library": "^0.2.0",
        "@types/react": "^18.0.28",
        "@types/react-dom": "^18.0.11",
        "@types/wordpress__blocks": "^12.0.3",
        "@types/wordpress__components": "^23.0.1",
        "@wordpress/scripts": "^25.5.1",
        "chalk": "^4.1.2",
        "concurrently": "^8.2.0",
        "dotenv": "^16.0.3",
        "inquirer": "^8.0.0",
        "minimist": "^1.2.8",
        "prettier": "^2.2.1",
        "scripty": "^2.0.0",
        "storybook": "^7.2.2",
        "tsconfig-paths-webpack-plugin": "^4.0.1"
    }
}
